 <div class="new-tank">
    <h1>Add a tank</h1>
    <form action="/tank/create" method="POST" class="enter-new-tank">
      <label for="name">Tank name</label>
      <input type="text" name="name">
      <label for="gallons_size">Size (in gallons)</label>
      <input type="text" name="gallons_size">
      <input type="submit">
    </form>
  </div>

  <script>
 $('body').on('submit', '.enter-new-tank', function(event) {
   event.preventDefault();
   $.ajax({
     url: $(this).attr('action'),
     type: 'POST',
     data: $(this).serialize(),
   })
   .done(function(data) {
    debugger
     console.log("success");
   })


 });
  </script>